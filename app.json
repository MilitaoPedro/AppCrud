{
  "expo": {
    "name": "appPedro",
    "slug": "appPedro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/compLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/compLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C1F3F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/compLogo.png",
        "backgroundColor": "#0C1F3F"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/compLogo.png"
    },
    "plugins": [
      "expo-router"    
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
